{
    "ComponentName": "position_cell",
    "BlockType": "text",
    "Source": "{Item.Position}",
    "MaxWidth": 66,
    "StretchWidth": true,
    //"Height": 15,
    "MaxHeight": 72,
    "Background": "{BackgroundDefault}",
    "Padding": "25,16,10,16",
    "FontName": "{FontNameDefault}",
    "FontSize": 14,
    "TextOptions": {
        "TextAlignment": "Start",
        "HorizontalAlignment": "Left"
    },
    "Triggers": [
        {
            "Trigger": "DarkerBackground"
        },
        {
            "Condition": "{Item.IntervalRace, Converter=StringEquals, Parameter=DNF}",
            "Property": "Source",
            "Value": "NC"
        },
        {
            "Condition": "{Item.IntervalRace, Converter=StringEquals, Parameter=DNS}",
            "Property": "Source",
            "Value": "DNS"
        },
        {
            "Condition": "{Item.IntervalRace, Converter=StringEquals, Parameter=DSQ}",
            "Property": "Source",
            "Value": "DSQ"
        }
    ]
}