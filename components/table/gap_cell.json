{
    "ComponentName": "gap_cell",
    "BlockType": "dock",
    "Background": "{BackgroundDefault}",
    "MaxWidth": 215,
    "StretchWidth": true,
    //"Height": 15,
    "MaxHeight": 72,
    "Triggers": [
        {
            "Trigger": "DarkerBackground"
        }
    ],
    "Items": [
        {
            "BlockType": "text",
            "Width": 125,
            "Source": "{Item.Time}",
            "Style": "mainTable_Style",
            "Triggers": [
                {
                    "Trigger": "DarkerBackground"
                },
                {
                    "Condition": "{Item.Index, Converter=NumberEquals, Parameter=0}",
                    "Property": "Source",
                    "Value": ""
                },
                {
                    "Condition": "{Item.Time, Converter=StringEquals, Parameter=DSQ}",
                    "Property": "Source",
                    "Value": ""
                },
                {
                    "Condition": "{Item.Time, Converter=StringEquals, Parameter=DNF}",
                    "Property": "Source",
                    "Value": ""
                },
                {
                    "Condition": "{Item.Time, Converter=StringEquals, Parameter=DNS}",
                    "Property": "Source",
                    "Value": ""
                }
            ]
        }
    ]
}