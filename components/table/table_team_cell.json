{
    "ComponentName": "team_cell",
    "BlockType": "dock",
    "RenderIf": "{Season.LineupsBasedType, Converter=StringEquals, Parameter=Teams}",
    "Background": "{BackgroundDefault}",
    "MaxWidth": 230,
    "StretchWidth": true,
    //"Height": 15,
    "MaxHeight": 72,
    "Triggers": [
        {
            "Trigger": "DarkerBackground"
        }
    ],
    "Items": [
        {
            "BlockType": "text",
            "Width": 200,
            "Source": "{Item.Team.FullName}",
            "Style": "mainTable_Style",
            "Triggers": [
                {
                    "Condition": "{Season.LineupsBasedType, Converter=StringEquals, Parameter=Teams}",
                    "ConditionAnd": "{Item.Team.Name, Converter=EmptyObjectToTrue}",
                    "Property": "Source",
                    "Value": "RESERVE"
                }
            ]
        }
    ]
}