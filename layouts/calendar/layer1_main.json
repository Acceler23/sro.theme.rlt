{
    "BlockRoot": {
        "Name": "mainStack",
        "BlockType": "stack",
        "Orientation": "Vertical",
        "Items": [
            {
                "BlockType": "component",
                "Component": "league_head_cell"
            },
            {
                "BlockType": "stack",
                "Orientation": "Vertical",
                "Width": 1420,
                "Margin": "{MarginRoot}",
                "Items": [
                    //Head
                    {
                        "BlockType": "text",
                        "PaddingTop": 5,
                        "Source": "{Season.Name, Converter=StringToUpperString} CALENDAR",
                        "FontSize": 40,
                        "MarginBottom": 20
                    },
                    {
                        "BlockType": "stack",
                        "RenderIf": "{Season.SeasonCurrentNumberEvent, Converter=NumberGreater, Parameter=0}",
                        "Items": [
                            {
                                "BlockType": "text",
                                "Source": ">",
                                "FontSize": 24,
                                "PaddingTop": 5,
                                "Foreground": "{red}"
                            },
                            {
                                "BlockType": "text",
                                "Source": " PAST EVENTS",
                                "PaddingTop": 5,
                                "FontSize": 24
                            }
                        ]
                    },
                    //Past
                    {
                        "BlockType": "itemstack",
                        "Orientation": "Vertical",
                        "Width": 1460,
                        "ItemStackOptions": {
                            "ItemSource": "{Events.Events}",
                            "ItemTemplate": {
                                "BlockType": "dock",
                                "Orientation": "Horizontal",
                                "Background": "{BackgroundDefault}",
                                "Width": "*",
                                "Height": "*",
                                "Spacing": 200,
                                "Padding": "25,25,10,25",
                                "MarginTop": 25,
                                "Items": [
                                    {
                                        "BlockType": "component",
                                        "Component": "past_event_track"
                                    },
                                    {
                                        "BlockType": "text",
                                        "HorizontalAlignment": "Right",
                                        "Source": "{Item.WinnerString}",
                                        "VerticalAlignment": "Center",
                                        "PaddingTop": 5,
                                        "FontSize": 16
                                    }
                                ]
                            }
                        }
                    }
                ]
            },
