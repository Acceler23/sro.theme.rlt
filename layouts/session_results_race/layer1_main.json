{
    "BlockRoot": {
        "Name": "mainStack",
        "BlockType": "stack",
        "Orientation": "Vertical",
        "Items": [
            {
                "BlockType": "component",
                "Component": "league_head_cell"
            },
            {
                "BlockType": "stack",
                "Margin": "{MarginRoot}",
                "Orientation": "Vertical",
                "Items": [
                    {
                        "BlockType": "component",
                        "Component": "session_header"
                    },
                    {
                        "BlockType": "component",
                        "Component": "main_head_cell"
                    },
                    {
                        "BlockType": "component",
                        "Component": "head_background_cell"
                    },
                    //table
                    {
                        "Name": "mainTable",
                        "BlockType": "table",
                        "Margin": "{MarginTable}",
                        "Triggers": [
                            {
                                "Trigger": "RenderPages_Trigger"
                            }
                        ],
                        "TableOptions": {
                            "ItemsSource": "{Session.Drivers}",
                            "RowSpacing": 3,
                            "ColSpacing": 0,
                            "HeaderTemplate": {
                                "BlockType": "text",
                                "Style": "table_head_style",
                                "HorizontalAlignment": "Left"
                            },
                            "Columns": [
                                //position
                                {
                                    "Header": "     POS",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "position_cell"
                                    }
                                },
                                //Car Number
                                {
                                    "Header": "CAR #",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "number_cell"
                                    }
                                },
                                //Class
                                {
                                    "Header": "CLASS",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "class_cell"
                                    }
                                },
                                //Driver Name
                                {
                                    "Header": "DRIVERS",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "driver_cell"
                                    }
                                },
                                //Team
                                {
                                    "Header": "TEAM",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "team_cell"
                                    }
                                },
                                {
                                    "Header": "CAR",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "car_cell"
                                    }
                                },
                                //Fastest Lap
                                {
                                    "Header": "TIME",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "fastlap_race"
                                    }
                                },
                                //Laps
                                {
                                    "Header": "LAPS",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "Laps_Race"
                                    }
                                },
                                {
                                    "Header": "GAP",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "gap_cell"
                                    }
                                },
                                {
                                    "Header": "",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "fader_cell"
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        ]
    }
}