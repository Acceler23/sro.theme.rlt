                                                "data": "Item.Value9"
                                            }
                                        }
                                    }
                                },

                                //column 10
                                {
                                    "Header": "{Statistics.Column10.Header, Converter=StringToUpperString}",
                                    "IsSeparator": "{Statistics.Column10.Type, Converter=EnumEquals, Parameter=Separator}",
                                    "GroupId": "{Statistics.Column10.GroupId}",
                                    "WidthStretch": true,
                                    "Template": {
                                        "BlockType": "component",
