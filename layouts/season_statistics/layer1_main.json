{
    "BlockRoot": {
        "Name": "mainStack",
        "BlockType": "stack",
        "Orientation": "Vertical",
        "Items": [
            {
                "BlockType": "component",
                "Component": "league_head_cell"
            },
            {
                "BlockType": "stack",
                "Margin": "{MarginRoot}",
                "Orientation": "Vertical",
                "Items": [
                    {
                        "BlockType": "component",
                        "Component": "subheader_tables",
                        "ComponentOptions": {
                            "Vars": {
                                "renderData": "{Season.Name, Converter=StringToUpperString}  STATISTICS  |   {Season.LeagueInfo.LeagueName, Converter=StringToUpperString} DRIVERS"
                            }
                        }
                    },
                    {
                        "BlockType": "component",
                        "Component": "main_head_cell_season"
                    },
                    {
                        "BlockType": "component",
                        "Component": "head_background_cell"
                    },
                    //table
                    {
                        "Name": "mainTable",
                        "BlockType": "table",
                        "Margin": "{MarginTable}",
                        "Triggers": [
                            {
                                "Trigger": "RenderPages_Trigger"
                            }
                        ],
                        "TableOptions": {
                            "ItemsSource": "{Statistics.Drivers}",
                            "RowSpacing": 3,
                            "ColSpacing": 0,
                            "HeaderTemplate": {
                                "BlockType": "text",
                                "Style": "table_head_style",
                                "HorizontalAlignment": "Left"
                            },
                            "Columns": [
                                //position
                                {
                                    "Header": "     POS",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "position_cell"
                                    }
                                },
                                //Car Number
                                {
                                    "Header": "CAR #",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "number_cell"
                                    }
                                }, 
                                //Class
                                {
                                    "Header": "CLASS",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "class_cell"
                                    }
                                },
                                //Driver Name
                                {
                                    "Header": "DRIVERS",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "driver_cell"
                                    }
                                },
                                //Team
                                {
                                    "Header": "TEAM",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "team_cell"
                                    }
                                },
                                {
                                    "Header": "CAR",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "car_cell"
                                    }
                                },
                                //column 0
                                {
                                    "Header": "{Statistics.Column0.Header, Converter=StringToUpperString}",
                                    "IsSeparator": "{Statistics.Column0.Type, Converter=EnumEquals, Parameter=Separator}",
                                    "GroupId": "{Statistics.Column0.GroupId}",
                                    "WidthStretch": true,
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "session_stats_cell",
                                        "ComponentOptions": {
                                            "Vars": {
                                                "data": "Item.Value0"
                                            }
                                        }
                                    }
                                },

                                //column 1
                                {
                                    "Header": "{Statistics.Column1.Header, Converter=StringToUpperString}",
                                    "IsSeparator": "{Statistics.Column1.Type, Converter=EnumEquals, Parameter=Separator}",
                                    "GroupId": "{Statistics.Column1.GroupId}",
                                    "WidthStretch": true,
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "session_stats_cell",
                                        "ComponentOptions": {
                                            "Vars": {
                                                "data": "Item.Value1"
                                            }
                                        }
                                    }
                                },

                                //column 2
                                {
                                    "Header": "{Statistics.Column2.Header, Converter=StringToUpperString}",
                                    "WidthStretch": true,
                                    "IsSeparator": "{Statistics.Column2.Type, Converter=EnumEquals, Parameter=Separator}",
                                    "GroupId": "{Statistics.Column2.GroupId}",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "session_stats_cell",
                                        "ComponentOptions": {
                                            "Vars": {
                                                "data": "Item.Value2"
                                            }
                                        }
                                    }
                                },

                                //column 3
                                {
                                    "Header": "{Statistics.Column3.Header, Converter=StringToUpperString}",
                                    "WidthStretch": true,
                                    "IsSeparator": "{Statistics.Column3.Type, Converter=EnumEquals, Parameter=Separator}",
                                    "GroupId": "{Statistics.Column3.GroupId}",
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "session_stats_cell",
                                        "ComponentOptions": {
                                            "Vars": {
                                                "data": "Item.Value3"
                                            }
                                        }
                                    }
                                },

                                //column 4
                                {
