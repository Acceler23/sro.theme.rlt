                                    "IsSeparator": "{Statistics.Column5.Type, Converter=EnumEquals, Parameter=Separator}",
                                    "GroupId": "{Statistics.Column5.GroupId}",
                                    "WidthStretch": true,
                                    "Template": {
                                        "BlockType": "component",
                                        "Component": "session_stats_cell",
                                        "ComponentOptions": {
                                            "Vars": {
