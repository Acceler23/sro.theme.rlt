{
    "BlockRoot": {
        "Name": "mainStack",
        "BlockType": "stack",
        "Margin": "{MarginRoot}",
        "Orientation": "Vertical",
        "Items": [
            {
                "BlockType": "component",
                "Component": "league_head_cell"
            },
            {
                "BlockType": "stack",
                "Orientation": "Horizontal",
                "Padding": "{PaddingDefault}",
                "HorizontalAlignment": "Left",
                "MarginBottom": 10,
                "Items": [
                    {
                        "BlockType": "stack",
                        "Items": [
                            {
                                "BlockType": "text",
                                "Source": ">",
                                "FontSize": 24,
                                "Foreground": "{HeadMain}"
                            },
                            {
                                "BlockType": "text",
                                "HorizontalAlignment": "Right",
                                "Source": " {Season.Name}  |  Reserves",
                                "FontSize": 24
                            }
                        ]
                    }
                ]
            },
            {
                "BlockType": "itemstack",
                "Orientation": "Horizontal",
                "Width": "*",
                "ItemStackOptions": {
                    "ItemSource": "{Lineups.LineupsReserves}",
                    "CollectionPart": "1/3",
                    "ItemTemplate": {
                        "BlockType": "stack",
                        "Orientation": "Vertical",
                        "MarginRight": 40,
                        "Items": [
                            //team
                            {
                                "BlockType": "stack",
                                "HorizontalAlignment": "Center",
                                "VerticalAlignment": "Center",
                                "Orientation": "Vertical",
                                "Background": "{BackgroundDarker}",
                                "Items": [
                                    //Driver
                                    {
                                        "BlockType": "component",
                                        "Component": "ln_driver_reserve"
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            {
                "BlockType": "itemstack",
                "Orientation": "Horizontal",
                "Width": "*",
                "ItemStackOptions": {
                    "ItemSource": "{Lineups.LineupsReserves}",
                    "CollectionPart": "2/3",
                    "ItemTemplate": {
                        "BlockType": "stack",
                        "Orientation": "Vertical",
                        "MarginRight": 40,
                        "Items": [
                            //team
                            {
                                "BlockType": "stack",
                                "HorizontalAlignment": "Center",
                                "VerticalAlignment": "Center",
                                "Orientation": "Vertical",
                                "Background": "{BackgroundDarker}",
                                "Items": [
                                    //Driver
                                    {
                                        "BlockType": "component",
                                        "Component": "ln_driver_reserve"
                                    }
                                ]
                            }
                        ]
                    }
                }
            },
            {
                "BlockType": "itemstack",
                "Orientation": "Horizontal",
                "Width": "*",
                "ItemStackOptions": {
                    "ItemSource": "{Lineups.LineupsReserves}",
                    "CollectionPart": "3/3",
                    "ItemTemplate": {
                        "BlockType": "stack",
                        "Orientation": "Vertical",
                        "MarginRight": 40,
                        "Items": [
                            //team
                            {
                                "BlockType": "stack",
                                "HorizontalAlignment": "Center",
                                "VerticalAlignment": "Center",
                                "Orientation": "Vertical",
                                "Background": "{BackgroundDarker}",
                                "Items": [
                                    //Driver
                                    {
                                        "BlockType": "component",
                                        "Component": "ln_driver_reserve"
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        ]
    }
}