                    {
                        "BlockType": "itemstack",
                        "Orientation": "Vertical",
                        "Width": "*",
                        "ItemStackOptions": {
                            "ItemSource": "{Lineups.LineupsReserves}",
                            "CollectionPart": "3/3",
