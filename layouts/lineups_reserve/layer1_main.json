{
    "BlockRoot": {
        "Name": "mainStack",
        "BlockType": "stack",
        "Margin": "{MarginRoot}",
        "Orientation": "Vertical",
        "Items": [
            {
                "BlockType": "component",
                "Component": "league_head_cell"
            },
            {
                "BlockType": "stack",
                "Orientation": "Horizontal",
                "Padding": "{PaddingDefault}",
                "HorizontalAlignment": "Left",
                "MarginBottom": 10,
                "Items": [
                    {
                        "BlockType": "stack",
                        "Items": [
                            {
                                "BlockType": "text",
                                "Source": ">",
                                "FontSize": 24,
                                "Foreground": "{HeadMain}",
                                "PaddingTop": 5
                            },
                            {
                                "BlockType": "text",
                                "HorizontalAlignment": "Right",
                                "Source": " {Season.Name}  |  Reserves",
                                "FontSize": 24,
                                "PaddingTop": 5
                            }
                        ]
                    }
                ]
            },
            {
                "BlockType": "stack",
                "Orientation": "Horizontal",
                "Spacing": 40,
                "Items": [
                    {
                        "BlockType": "itemstack",
                        "Orientation": "Vertical",
                        "Width": "*",
                        "ItemStackOptions": {
                            "ItemSource": "{Lineups.LineupsReserves}",
                            "CollectionPart": "1/3",
                            "ItemTemplate": {
                                "BlockType": "stack",
                                "Orientation": "Vertical",
                                "MarginTop": 10,
                                "Items": [
                                    //team
                                    {
                                        "BlockType": "stack",
                                        "HorizontalAlignment": "Center",
                                        "VerticalAlignment": "Center",
                                        "Orientation": "Vertical",
                                        "Background": "{BackgroundDarker}",
                                        "Items": [
                                            //Driver
                                            {
                                                "BlockType": "component",
                                                "Component": "driver_card",
                                                "ComponentOptions": {
                                                    "Vars": {
                                                        "isReserve": true,
                                                        "isReserveReverse": false
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "BlockType": "itemstack",
                        "Orientation": "Vertical",
                        "Width": "*",
                        "ItemStackOptions": {
                            "ItemSource": "{Lineups.LineupsReserves}",
                            "CollectionPart": "2/3",
